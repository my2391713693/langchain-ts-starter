{
  "name": "langchain-ts-starter",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "tsup": "tsup",
    "build": "tsup",
    "start": "tsx -r dotenv/config src/index.ts",
    "start:vector": "tsx -r dotenv/config src/vectorStoreExample.ts",
    "start:server": "tsx -r dotenv/config src/server.ts",
    "dev:server": "tsx watch -r dotenv/config src/server.ts",
    "start:chroma": "bash scripts/start-chroma.sh",
    "start:server:auto": "tsx -r dotenv/config src/server.ts",
    "lint": "eslint src",
    "lint:fix": "npm run lint --fix",
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "prettier --list-different \"**/*.ts\""
  },
  "keywords": [
    "langchain",
    "starter",
    "template",
    "node",
    "typescript",
    "llm"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@langchain/community": "^1.0.4",
    "@langchain/openai": "^1.1.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "axios": "^1.13.2",
    "chromadb": "^1.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^5.1.0",
    "hnswlib-node": "^1.4.2",
    "langchain": "^0.0.39",
    "node-fetch": "^2.7.0",
    "tsup": "^6.7.0"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.2",
    "@types/js-yaml": "^4",
    "@types/node": "^18.15.5",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.3",
    "tsx": "^3.12.3",
    "typescript": "^5.0.4"
  }
}
